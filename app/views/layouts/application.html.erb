    <style>
      /* .container-fluid {
        padding: 0 0;
      } */
      a:hover{
        text-decoration: none;
      }
      .app-name a {
        padding-bottom: 100;
      }

    </style>

  </head>

  <body>
      <%= yield %>
  </body>
